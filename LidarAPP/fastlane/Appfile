# Appfile for LiDAR 3D Scanner iOS App
# https://docs.fastlane.tools/advanced/Appfile/

# App Identifier (Bundle ID)
app_identifier("com.lidarscanner.app")

# Your Apple Developer email address
apple_id("YOUR_APPLE_ID@email.com")

# Team ID from Apple Developer Portal
# Find at: https://developer.apple.com/account → Membership → Team ID
team_id("YOUR_TEAM_ID")

# iTunes Connect Team ID (usually same as team_id)
# Required for upload_to_testflight and upload_to_app_store
itc_team_id("YOUR_TEAM_ID")

# For App Store Connect API Key (recommended over Apple ID)
# Generate at: App Store Connect → Users and Access → Keys → App Store Connect API
#
# Uncomment and fill in to use API Key authentication:
# app_store_connect_api_key(
#   key_id: "YOUR_KEY_ID",
#   issuer_id: "YOUR_ISSUER_ID",
#   key_filepath: "./AuthKey_XXXXXXXX.p8",
#   duration: 1200,
#   in_house: false
# )
